			SigesoftEntitiesModel dbContext = new SigesoftEntitiesModel();
			var query = (from sc in dbContext.servicecomponent
						 join c in dbContext.component on sc.v_ComponentId equals c.v_ComponentId
						 where (sc.i_IsDeleted == 0) &&
