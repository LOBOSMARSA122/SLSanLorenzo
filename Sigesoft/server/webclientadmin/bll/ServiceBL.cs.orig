                                v_DiseasesName = ddd.v_Name,
                                v_CreationUser = J1.v_UserName,
                                v_UpdateUser = J2.v_UserName,
                                d_CreationDate = J1.d_InsertDate,
                                d_UpdateDate = J2.d_UpdateDate,
