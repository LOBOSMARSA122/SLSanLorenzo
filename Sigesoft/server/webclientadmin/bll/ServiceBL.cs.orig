                                item.d_ApprovedInsertDate = DateTime.Now;
                                item.i_IsApprovedId = pobjDtoEntity.i_IsApprovedId;
                                //Para que salga la firma
                                item.i_ApprovedUpdateUserId = Int32.Parse(ClientSession[2]);
                            }
                            else
                            {
