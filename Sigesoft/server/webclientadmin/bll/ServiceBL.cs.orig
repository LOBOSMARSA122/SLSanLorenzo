                          }).ToList();

                // workstationdangers
                var q3 = (from A in historyId
                          select new Sigesoft.Node.WinClient.BE.PersonMedicalHistoryList
                          {
                              v_AntecedentTypeName = "Ocupacionales, " + A.v_Occupation,
